{
    "name":         "jms/job-queue-bundle",
    "description":  "Convertr fork for Symfony 4+ compatibility - Allows to run and schedule Symfony console commands as background jobs.",
    "keywords":     ["job","queue", "background", "cron", "scheduled"],
    "homepage":     "http://jmsyst.com/bundles/JMSJobQueueBundle",
    "type":         "symfony-bundle",
    "license":      "Apache-2.0",
    "authors": [
        {
            "name":   "Johannes M. Schmitt",
            "email":  "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "doctrine/common": "^2.3 || ^3.0",
        "symfony/debug": "^4.4",
        "symfony/error-handler": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "symfony/console": "^4.4 || ^5.0",
        "symfony/process": "^4.4 || ^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "doctrine/annotations": "1.11.1",
        "doctrine/doctrine-bundle": "1.12.2",
        "doctrine/dbal": "2.13.9",
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "doctrine/orm": "2.7.5",
        "phpunit/phpunit": "8.2.3",
        "sensio/framework-extra-bundle": "6.2.10",
        "symfony/browser-kit": "4.4.44",
        "symfony/class-loader": "3.4.47",
        "symfony/css-selector": "4.4.44",
        "symfony/finder": "4.4.44",
        "symfony/form": "4.4.48",
        "symfony/intl": "4.4.47",
        "symfony/twig-bundle": "4.4.41",
        "symfony/validator": "4.4.48",
        "symfony/yaml": "4.4.45"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "Required when using the webinterface.",
        "symfony/twig-bundle": "Required when using the webinterface."
    },
    "autoload": {
        "psr-4": { "JMS\\JobQueueBundle\\": "." }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
